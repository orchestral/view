{
    "name": "orchestra/view",
    "description": "View Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/view/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "view", "theme"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\View\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\View\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/view": "dev-master",
        "orchestra/contracts": "dev-master",
        "orchestra/memory": "dev-master",
        "orchestra/support": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
