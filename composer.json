{
	"name": "orchestra/view",
	"description": "Orchestra Platform 2 View Component",
	"homepage": "http://orchestraplatform.com/docs/latest/components/view/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "view", "theme"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\View\\" : "src/View/"
		}
	},
	"require": {
		"php": ">=5.3.3",
		"orchestra/memory": "2.1.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
        "illuminate/console": "4.1.*",
		"illuminate/container": "4.1.*",
		"illuminate/view": "4.1.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/view component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/view with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"minimum-stability": "dev"
}
